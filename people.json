---
layout: null
permalink: /people.json
---
[
{% assign last = site.people | size | minus: 1 %}
{% for p in site.people %}
  {
    "id": {{ p.data["pid"] | jsonify }},
    "name": {{ p.data["name"] | jsonify }},
    "given_names": {{ p.data["given_names"] | jsonify }},
    "family_name": {{ p.data["family_name"] | jsonify }},
    "aliases": {{ p.data["aliases"] | jsonify }},
    "gender": {{ p.data["gender"] | jsonify }},
    "born": {{ p.data["born"] | jsonify }},
    "born_place": {{ p.data["born_place"] | jsonify }},
    "born_place_id": {{ p.data["born_place_id"] | jsonify }},
    "died": {{ p.data["died"] | jsonify }},
    "died_place": {{ p.data["died_place"] | jsonify }},
    "died_place_id": {{ p.data["died_place_id"] | jsonify }},
    "parents": {{ p.data["parents"] | jsonify }},
    "spouses": {{ p.data["spouses"] | jsonify }},
    "photo": {{ p.data["photo"] | jsonify }},
    "privacy": {{ p.data["privacy"] | jsonify }},
    "career": {{ p.data["career"] | jsonify }},
    "education": {{ p.data["education"] | jsonify }},
    "events": {{ p.data["events"] | jsonify }},
    "tags": {{ p.data["tags"] | jsonify }}
  }{% if forloop.index0 < last %},{% endif %}
{% endfor %}
]
